{
  "restartable": "rs",
  "ignore": [
    ".git",
    "f2e",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
  ],
  "env": {
    "NODE_ENV": "development",
    "DB_HOST": "localhost",
    "DB_USERNAME": "root",
    "DB_NAME": "stock",
    "DB_PASSWORD": ""
  },
  "ext": "js,json"
}
